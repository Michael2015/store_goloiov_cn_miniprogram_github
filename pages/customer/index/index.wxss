.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.modal-dialog {
  width: 638rpx;
  position: fixed;
  top: 6%;
  left: 6%;
  z-index: 9999;
}
.modal-dialog image
{
  width:638rpx;
  height: 826rpx;
}
.modal-title {
  color: #030303;
  text-align: center;
  padding: 15rpx 0;
}
.modal-content {
  position: absolute;
  width: 638rpx;
  text-align: center;
  top: 55%;
  color: #fff;
  font-weight: bold;
  height: 160rpx;
}
.modal-content .nav{
    width: 60%;
    margin: 0 auto;
}
