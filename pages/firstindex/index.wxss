.bannerPic{
  height: 360rpx;
}
.bannerPic image{
  width: 100%;
  height: 100%;
}
.head{
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 40rpx 0 20rpx;
  font-size: 32rpx;
}
.head text:nth-of-type(2){
  color: blue;
}
.carinfo{
  margin: 0 20rpx;
  display: flex;
  height: 156rpx;
  background: rgba(242, 242, 242, 1);
}
.left,.right{
  width: 50%;
  height: 100%;
}
.left{
    padding: 28rpx 20rpx;

}
.left view{
  line-height: 50rpx;
}
.right{
   text-align: right;
   line-height: 156rpx;
   padding-right: 20rpx;
}
.import{
  margin: 20rpx;
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.import view image{
  width: 94rpx;
  height: 86rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}
.import view{
  width: 49%;
  height: 124rpx;
  line-height: 124rpx;
  background: pink;
  margin-bottom: 2%;
  text-align: center;
}
.import view:nth-child(2n-1){
  margin-right: 1%;
}
.import view:nth-child(2n){
  margin-left: 1%;
}
.chepu{
    text-align: center;
    position: relative;
    height: 146rpx;
    margin: 19rpx 19rpx 0;
  }
  .chepu image{
    width:100%;
    height: 100%;
  }
  .chepu_posi{
    position: absolute;
    left: 0;
    top: 50%;
    transform: translate(0,-50%);
    width: 100%;
