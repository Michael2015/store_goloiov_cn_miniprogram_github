.wrap {
  height: 100%;
  background: rgba(249, 249, 249, 1);
  padding-bottom: 172rpx;
}

.head {
  height: 340rpx;
  text-align: center;
  line-height: 340rpx;
  margin-bottom: 30rpx;
  background: white;
}

.head image {
  width: 685rpx;
  height: 280rpx;
  vertical-align: middle;
}

.list {
  height: 120rpx;
  line-height: 120rpx;
  border-bottom: 1rpx solid rgba(216, 216, 216, 1);
  padding: 0 30rpx;
  background: white;
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.list text:nth-of-type(2){
color:rgba(153,153,153,1);
}
.last {
  border: none;
  margin-top: 30rpx;
}
.last input{
  text-align: right;
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.btn {
  width: 556rpx;
  height: 92rpx;
  line-height: 92rpx;
  margin-top: 60rpx;
  background:rgba(190,0,0,1);
border-radius:10rpx;
font-size:30rpx;
font-family:PingFangSC-Medium,PingFang SC;
font-weight:500;
color:rgba(255,255,255,1);
}
