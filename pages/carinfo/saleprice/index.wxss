.index{
  height: 100vh;
  background: url("https://wcp.wanchepin.com/public/wap/x431/car_bg.png") no-repeat center;
  background-size: cover;
  overflow: hidden;
  padding:43rpx 32rpx 32rpx;
  box-sizing: border-box;
}
.wrap{
  position: relative;
}
.con1,.con2{
  width: 32rpx;
  height: 81rpx;
   position: absolute;
top: 225rpx;
}
.con1{
 left: 32rpx;
}
.con2{
  right: 32rpx;
}
.first{
height: 250rpx;
background: white;
border-radius: 10rpx;
padding: 50rpx 32rpx 0;
box-sizing: border-box;
overflow: hidden;
}
.first1{
  height:60rpx;
  display: flex;
justify-content: space-between;
align-items: center;
}
.first1 view:nth-of-type(1){
  font-size:42rpx;
font-family:PingFangSC-Medium,PingFang SC;
font-weight:500;
color:rgba(113,46,46,1);
}
.first1 view:nth-of-type(2){
  line-height: 60rpx;
}
.first1 view:nth-of-type(2) text{
font-size:30rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(74,94,226,1);
}
.first1 view:nth-of-type(2) image{
width: 24rpx;
height: 24rpx;
margin-right: 10rpx;
vertical-align: middle;
margin-top: -4rpx;
}
.first2{
  height:42rpx;
font-size:30rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(113,46,46,1);
line-height:42rpx;
margin-top: 30rpx;
}
.first2 text:nth-of-type(2){
  margin: 0 25rpx;
}
.second{
margin-top: 32rpx;
height: 350rpx;
background: white;
border-radius: 10rpx;
overflow: hidden;
}
.second1{
height:42rpx;
font-size:30rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(113,46,46,1);
line-height:42rpx;
margin-top: 70rpx;
text-align: center;
}
.second2{
  text-align: center;
  margin-top: 40rpx;
}
.second2 text:nth-of-type(1){
height:120rpx;
font-size:100rpx;
font-family:BarlowSemiCondensed-SemiBold,BarlowSemiCondensed;
font-weight:600;
color:rgba(113,46,46,1);
letter-spacing:2rpx;
}
.second2 text:nth-of-type(2){
font-size:30rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(113,46,46,1);
line-height:42rpx;
margin-top: 70rpx;
text-align: center;
}