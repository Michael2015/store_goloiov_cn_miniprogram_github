.index{
  background:rgba(249,249,249,1);
  height: 100vh;
}
.list {
  height: 120rpx;
  line-height: 120rpx;
  border-bottom: 1rpx solid rgba(216,216,216,1);
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 30rpx;
  font-size:30rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
}
.list text:nth-of-type(1){
color: #666666;
}
.list text:nth-of-type(2){
  color:#333333;
}
.list input{
  text-align: right;
}
.btn {
width:556rpx;
height:92rpx;
line-height: 92rpx;
background:rgba(190,0,0,1);
border-radius:10rpx;
font-size:30rpx;
font-family:PingFangSC-Medium,PingFang SC;
font-weight:500;
color:rgba(255,255,255,1);
margin-top: 60rpx;
}