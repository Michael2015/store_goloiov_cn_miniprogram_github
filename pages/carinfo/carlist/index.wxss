.list{
  height: 146rpx;
  border-bottom: 1rpx solid rgba(221,221,221,1);
  display: flex;
  justify-content: space-between;
  padding: 23rpx 30rpx;
  box-sizing: border-box;
}
.left,.right{
    height: 100%;
    box-sizing: border-box;
}
.left{
  width: 60%;
}
.left view:nth-of-type(1){
  font-size:32rpx;
font-family:PingFangSC-Medium,PingFang SC;
font-weight:500;
color:rgba(51,51,51,1);
}
.left view:nth-of-type(2){
  font-size:30rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(102,102,102,1);
}
.left view{
  line-height: 50rpx;
}
.left .nofirst{
  margin-right: 10rpx;
}

.right{
  width: 40%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:30rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(102,102,102,1);
}
.right view:nth-of-type(1){
  color:rgba(74,94,226,1);
  margin-right: 40rpx;
}
.right image{
  width: 24rpx;
  height: 24rpx;
  margin-right: 9rpx;
}

