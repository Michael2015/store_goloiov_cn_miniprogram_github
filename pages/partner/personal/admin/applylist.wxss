/* pages/partner/personal/admin/applyrecord.wxss */
page{
    background: #F5F5F5;
}
.record_warp{
    width: 100%;
    height: 100%;
}
.record_item{
    width: 750rpx;
    height: 130rpx;
    box-sizing: border-box;
    background-color: #ffffff;
    border: 1rpx solid #f5f5f5;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    padding: 21rpx 30rpx;
}
.top{
    display: flex;
    justify-content: space-between;
    font-size: 30rpx;
}
.bottom{
    display: flex;
    justify-content: space-between;
    font-size: 24rpx;
    color:#999999;
}
.address{
    color: #333333;
}
.apply{
    color: #000000;
}
.bottom .ing{
    color: #fe0000;
}