<!--pages/partner/personal/index.wxml-->
<view class="warp">
  <!-- /pages/partner/personal/partner/index -->
  <!-- /pages/partner/personal/data/index -->
<view class="header" bindtap="to" data-url="/pages/partner/personal/data/index">
  <view>
    <view class="avatar">
      <image src="{{avatar}}"></image>
    </view>
    <view class="info">
      <view class="name">{{name}}
        <image src="/assets/image/vip-{{partner_level}}.png" mode="widthFix"></image>
      </view>
      <view class="label">
        <text wx:if="{{server_vip}}">VIP服务商</text>
        <text wx:if="{{region_agent}}">区域管理员</text>
      <!-- <view class="num">伙伴{{num}}人</view> -->
      </view>
    </view>
    <view class="right">
      <image src="/assets/image/right-white.png" mode="widthFix"></image>
    </view>
  </view>
  <view class="circle"></view>
</view>
<view class="tabs" wx:if="{{partner_level_num>0}}">
  <view bindtap="to" data-url="/pages/partner/personal/visitor/index">
    <image src="/assets/image/visitor.png" mode="widthFix" class="visitor"></image>
    <text>访客</text>
  </view>
  <view class="partner-wrap" bindtap="to" data-url="/pages/partner/personal/partner/index">
    <image src="/assets/image/partner.png" mode="widthFix" class="partner"></image>
    <text>我的伙伴</text>
  </view>
</view>

<!--view class="contribution" bindtap="to" data-url="/pages/partner/personal/contribution/index">
    <image src="/assets/image/contribution.png" mode="widthFix"></image>
</view!-->
<view class="group">
  <view class="nav" bindtap="to" data-url="/pages/partner/personal/golo_points/index">
    <view class="icon"><image src="/assets/image/jifen2.png" mode="widthFix"></image></view>
    <view class="name">我的积分</view>
    <view class="right-text"></view>
    <view class="right-icon"><image src="/assets/image/right.png" mode="widthFix"></image></view>
  </view>
</view>
<view class="group">
  <view class="nav" bindtap="to" data-url="/pages/partner/personal/poster/index">
    <view class="icon"><image src="/assets/image/Slice1.png" mode="widthFix"></image></view>
    <view class="name">我的海报</view>
    <view class="right-text"></view>
    <view class="right-icon"><image src="/assets/image/right.png" mode="widthFix"></image></view>
  </view>
</view>
<view class="group">
  <view class="nav" bindtap="SeeFlag" data-url="/pages/partner/personal/admin/index">
    <view class="icon"><image src="/assets/image/Slice2.png" mode="widthFix"></image></view>
    <view class="name">区域管理员</view>
    <view class="right-text"></view>
    <view class="right-icon"><image src="/assets/image/right.png" mode="widthFix"></image></view>
  </view>
</view>
<view class="group">
  <view class="nav" bindtap="SeeServerFlag" data-url="/pages/partner/personal/servervip/index">
    <view class="icon"><image src="/assets/image/vip.png" mode="widthFix"></image></view>
    <view class="name">VIP服务商</view>
    <view class="right-text"></view>
    <view class="right-icon"><image src="/assets/image/right.png" mode="widthFix"></image></view>
  </view>
</view>
<view class="group">
  <view class="nav" bindtap="to" data-url="/pages/partner/personal/coupon/index">
    <view class="icon"><image src="/assets/image/my_coupon.png" mode="widthFix"></image></view>
    <view class="name">我的优惠券</view>
    <view class="right-text"></view>
    <view class="right-icon"><image src="/assets/image/right.png" mode="widthFix"></image></view>
  </view>
</view>
<view class="group">
  <view class="nav" bindtap="to" data-url="/pages/partner/shippingAddress/index">
    <view class="icon"><image src="/assets/image/address.png" mode="widthFix" class="address"></image></view>
    <view class="name">收货地址</view>
    <view class="right-text"></view>
    <view class="right-icon"><image src="/assets/image/right.png" mode="widthFix"></image></view>
  </view>
</view>
<view class="group">
  <view class="nav" bindtap="to" data-url="/pages/partner/comment/index">
    <view class="icon"><image src="/assets/image/home_Order.png" mode="widthFix"></image></view>
    <view class="name">我的评论</view>
    <view class="right-text"></view>
    <view class="right-icon"><image src="/assets/image/right.png" mode="widthFix"></image></view>
  </view>
</view>
