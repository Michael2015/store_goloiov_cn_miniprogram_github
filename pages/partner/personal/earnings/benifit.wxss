page{
    background: #f9f9f9;
}
.warp{
    font-size: 24rpx;
}
.warp .award{
    color: #fff;
    font-size: 34rpx;
    height: 163rpx;
    line-height: 163rpx;
    background: linear-gradient(90deg, 
    #f45445 0%, 
    #e62a21 100%);
    padding: 0 40rpx;
    font-family: PingFangSC-Medium;
}
.warp .simpleness_show:nth-child(1){
    margin-top: 20rpx;
}
.warp .simpleness_show{
    margin-top: 2rpx;
    font-size: 28rpx;
    color: #9e9e9e;
    height: 100rpx;
    line-height: 100rpx;
    padding: 0 30rpx;
    display: flex;
    background: #fff;
}
.warp .simpleness_show .right{
    color: #333333;
    flex: 1;
    text-align: right;
}

.warp .good_show{
    margin-top: 20rpx;
    background: #ffffff;
    padding: 25rpx 20rpx 20rpx 20rpx;
    display: flex;
}
.warp .good_show image{
    width: 138rpx;
    margin-right: 11rpx;
}
.warp .good_show .good_msg{
    flex:1;
}
.main_show{
    display: flex;
    border-bottom: 1rpx solid #f2f2f2;
    padding-bottom: 28rpx;
}
.main_show .desc{
    width: 432rpx;
    font-size: 26rpx;
    color: #333333;
}
.main_show .desc .color{
    color: #999999;
    font-size: 24rpx;
    margin-top: 16rpx;
}
.main_show .about_money{
    flex: 1;
    font-size: 24rpx;
    text-align: right;
}
.main_show .about_money .money{
    color: #000;
}
.main_show .about_money .count{
    color: #666666;
}
.good_msg .allmoney{
    text-align: right;
    height:66rpx;
    line-height: 66rpx;
}
.good_msg .allmoney span:nth-child(1){
    color: #333333;
    font-size: 22rpx;
    margin-right: 20rpx;
}
.good_msg .allmoney span:nth-child(2){
   display: inline-block;
   width: 2rpx;
   height: 23rpx;
   background: #cfcfcf;
   color: #cfcfcf;
}
.good_msg .allmoney span:nth-child(3){
    font-size: 24rpx;
    color: #333333;
    margin-left: 20rpx;
}
.good_msg .allmoney span:nth-child(3) >span{
    font-size: 24rpx;
	color: #e31436;
}
.exey_show_list{
    padding: 40rpx 30rpx;
    background: #fff;
    margin-top: 20rpx;
}
.exey_show_list .exey_show:nth-child(1){
    margin-top: 0;
}
.exey_show_list .exey_show{
    margin-top: 30rpx;
    height: 40rpx;
    line-height: 40rpx;
    display: flex;
    justify-content: space-between;
    font-size: 28rpx
}
.exey_show_list .exey_show .left{
    color: #9e9e9e;
}
.exey_show_list .exey_show .right{
    color: #333;
}