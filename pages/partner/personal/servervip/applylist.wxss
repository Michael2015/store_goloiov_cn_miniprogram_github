/* pages/partner/personal/servervip/applyrecord.wxss */
page{
    background: #F5F5F5;
}
.record_warp{
    width: 100%;
    height: 100%;
}
.record_item{
    width: 750rpx;
    height: 130rpx;
    box-sizing: border-box;
    background-color: #ffffff;
    border: 1rpx solid #f5f5f5;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    padding: 21rpx 30rpx;
}
.top{
    display: flex;
    justify-content: space-between;
    font-size: 28rpx;
}
.bottom{
    display: flex;
    justify-content: space-between;
    font-size: 28rpx;
    color:#999999;
}
.shop-name{
    color: #333333;
}
.apply{
    color: #000000;
}
.bottom .ing{
    color: #fe0000;
}

.no-more {
  text-align: center;
  color: #aaa;
  font-size: 28rpx;
  box-sizing: border-box;
  visibility:visible;
  margin-top: 20rpx;
}