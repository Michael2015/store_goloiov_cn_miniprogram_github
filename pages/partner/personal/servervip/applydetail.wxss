/* pages/partner/personal/servervip/applydetail.wxss */
page{
    background: #f5f5f5;
}
.detail-warp{
    width: 100%;
    height: 100vh;
    background:#fff;
}
.item{
    height: 84rpx;
    display: flex;
    justify-content: space-between;
    padding: 0 30rpx;
    align-items: center;
    font-size: 28rpx;
    background: #fff;
    border: 1rpx solid #f5f5f5;
}
.red{
    color:#fe0000;
}
.pink{
    color: #c4c4c6;
}

.shop-item {
    background: #fff;
    padding: 0 30rpx;
    border: 1rpx solid #f5f5f5;
}
.shop-item .detail{
    font-size:28rpx;
    margin-top: 22rpx;
    color: #c4c4c6;
    height: auto;
    padding: 10rpx 10rpx;
    word-wrap: break-word;
}

.shop-item label{
    font-size: 28rpx;
}

.upUpoto {
    padding-top: 20rpx;
    padding-left: 15rpx;
    display: flex;
}
.upUpoto .photo_title {
  height: 88rpx;
  display: flex;
  align-items: center;
  color: #666;
}

.upUpoto .photo_title .min {
  font-size: 20rpx;
  color: #999;
}
.add_img {
  width: 32rpx;
  height: 32rpx;
}
.user_select_photo{
    width: 100rpx;
    height: 100rpx;
}
